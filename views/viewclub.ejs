                                    <th>Description</th>
                                    <th>Link to Image</th>
                                    
                                    <th>Status</th>
                                    

                            
                                    <th>Action</th>
                                </tr>
                            </thead>
                            <tbody>
                                <% categories.forEach(function(data){ %>
                                <tr>
                                    <!-- <td><%= data.id %></td> -->
