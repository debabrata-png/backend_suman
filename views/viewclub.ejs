                                    <td><%= data.description %></td>
                                    <td><%= data.link %></td>
                                    
                                    <td><%= data.status1 %></td>
                                    
                                    
                                    <td>
                                        <a href="/editclub/<%= data.id %>" class="btn btn-primary"><i class="fa fa-edit"></i></a>
                                        <a href="/viewclubregistrations?clubcode=<%= data.id %>" class="btn btn-primary"><i class="fas fa-address-book" title="Enrolled student list"></i></a>
                                        <!-- <a href="http://naac2021.jdvu.ac.in/?criteria=1&Subsection=ValueAddedCourse&field1=<%= data.id %>" class="btn btn-primary" target="_blank"><i class="fas fa-upload"></i></a> -->
