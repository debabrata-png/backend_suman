<%- include("header") %> 



<!-- Content Header (Page header) -->
<div class="content-header">
    <div class="container-fluid">
        <div class="row mb-2">
            <!-- <div class="col-sm-6">
                <h1 class="m-0 text-dark">View Add on Course</h1>
            </div> -->
            <!-- /.col -->
            <div class="col-sm-8">
                <ol class="breadcrumb float-sm-left">
                    <li class="breadcrumb-item"><a href="/dashboard">Home</a></li>
                    <li class="breadcrumb-item"><a href="/viewaddoncourse">Value Added Courses</a></li>
                    <li class="breadcrumb-item active">List of Courses</li>
                </ol>
            </div><!-- /.col -->
        </div><!-- /.row -->
    </div><!-- /.container-fluid -->
</div>
<!-- /.content-header -->

<!-- Main content -->
<section class="content">
    <div class="container-fluid">
        <!-- Small boxes (Stat box) -->
        <div class="row">
            <div class="col-md-12">
                <!-- general form elements disabled -->
                <div class="card card-dark">
                    <div class="card-header">
                        <h3 class="card-title">View Value Added Courses</h3>
                    </div>
                    <!-- /.card-header -->
                    <div class="card-body">
                        <!-- <form method="post" action="/exportaddonc" id="frm-export-category"></form> -->
                        <table>
                            <tr>
                                <td>
                                    <a class="btn btn-dark" href="/createaddoncourse">+ Add New</a>
                                </td>
                                <td width="20px"></td>
                                <td>
                                    <a class="btn btn-dark" href="/addbulkaddonc">+ Bulk Upload</a>
                                </td>
                                <!-- <td width="20px"></td>
                                <td>
                                    <a href="javascript:void(0)" onclick="if(confirm('Are you sure want to export ? ')){  $('#frm-export-category').submit()  }" class="btn btn-dark">Export to excel</a>
                                </td> -->
                            </tr>
                        </table>
                        
                        <br><br>
                        <table id="tbl-list-category" class="table table-bordered table-striped">
                            <thead>
                                <tr>
                                    <th>Title of Add on Course</th>
                                    <th>Course Code</th>
                                    <th>Course Type</th>
                                    <th>Year of Offering</th>
                                    <th>No.of times course offered</th>
                                    <th>Duration</th>
                                    <th>No.of students enrolled</th>
                                    <th>No.of students completing the course</th>
                                    <th>Action</th>
                                </tr>
                            </thead>
                            <tbody>
                                <% categories.forEach(function(data){ %>

                                
                                <tr>
                                    <!-- <td><%= data.id %></td> -->
                                    <td><%= data.coursetitle %></td>
                                    <td><%= data.coursecode %></td>
                                    <td><%= data.coursetype %></td>
                                    <td><%= data.year %></td>
                                    <td><%= data.offeredtimes %></td>
                                    <td><%= data.duration %></td>
                                    <td><%= data.studentsenrolled %></td>
                                    <td><%= data.studentscompleted %></td>
                                    <td>
                                        <a href="/editaddoncourse/<%= data.id %>" class="btn btn-primary"><i class="fa fa-edit"title="Edit course"></i></a>
                                        <a href="/viewvacclass/<%= data.id %>" class="btn btn-primary"><i class="fab fa-wpforms" title="Add class"></i></a>
                                        <a href="/viewvacclassenroll/<%= data.id %>" class="btn btn-primary"><i class="fas fa-user-edit" title="Enroll students"></i></a>
                                        <!-- <a href="http://naac2021.jdvu.ac.in/?criteria=1&Subsection=ValueAddedCourse&field1=<%= data.id %>" class="btn btn-primary" target="_blank"><i class="fas fa-upload"></i></a> -->
                                        <% link.forEach(function(data1){ %>
                                            <a href="<%= data1.uploadlink %>?criteria=1&Subsection=ValueAddedCourse&field1=<%= data.id %>&Year=<%= data.yop %>" class="btn btn-primary" target="_blank"><i class="fas fa-upload"></i></a>


                                            <% }) %>
                                        <form method="post" action="/deleteaddoncourse" id="frm-delete-category-<%= data.id %>">
                                        
                                            <input type="hidden" value="<%= data.id %>" name="category_id"/>

                                        </form>

                                    <a href="javascript:void(0)" onclick="if(confirm('Are you sure want to delete ? ')){  $('#frm-delete-category-<%= data.id %>').submit()  }" class="btn btn-danger"><i class="fa fa-trash"></i></a>
                                    </td>
                                </tr>
                                <% }) %>
                            </tbody>
                            <!-- <tfoot>
                                <tr>
                                    <th>#ID</th>
                                    <th>Name</th>
                                    <th>Status</th>
                                    <th>Action</th>
                                </tr>
                            </tfoot> -->
                        </table>
                    </div>
                    <!-- /.card-body -->
                </div>
            </div>
        </div>
        <!-- /.row -->
    </div><!-- /.container-fluid -->
</section>
<!-- /.content -->

<%- include("footer") %> 

