<%- include("header") %> 

<div style="margin: 0px; padding: 40px;">
<h3>Add Course Outcome Calculation</h1>
<br>
<br>
<form id="frm-add-category" name="frm-add-category" method="post" action="/addcocal">
    <div class="row">
        <div class="col-sm-6">
            <!-- text input -->
            <div class="form-group">
                <label>Course Code</label>
                <input required type="text" class="form-control" placeholder="Enter course code" name="coursecode">
            </div>
        </div>
        <div class="col-sm-6">
            <!-- text input -->
            <div class="form-group">
                <label>Course Name </label>
                <input required type="text" class="form-control" placeholder="Enter course Name" name="course">
            </div>
        </div>
        <div class="col-sm-6">
            <!-- text input -->
            <div class="form-group">
                <label>Program Code</label>
                <input required type="text" class="form-control" placeholder="Enter Program code" name="programcode">
            </div>
        </div>
        <div class="col-sm-6">
            <!-- text input -->
            <div class="form-group">
                <label>Program</label>
                <input required type="text" class="form-control" placeholder="Enter the Program" name="program">
            </div>
        </div>

        <div class="col-sm-6">
            <!-- text input -->
            <div class="form-group">
                <label>Exam Code</label>
                <input required type="text" class="form-control" placeholder="Enter the Exam Code" name="examcode">
            </div>
        </div>
        
        <div class="col-sm-6">
            <!-- text input -->
            <div class="form-group">
                <label>Exam Name</label>
                <input required type="text" class="form-control" placeholder="Enter the Exam Name" name="examname">
            </div>
        </div>

        <div class="col-sm-6">
            <!-- text input -->
            <div class="form-group">
                <label>Semester</label>
                <input required type="text" class="form-control" placeholder="Enter the Semester" name="semester">
            </div>
        </div>

        <div class="col-sm-6">
            <!-- text input -->
            <div class="form-group">
                <label>IA Marks</label>
                <input required type="text" class="form-control" placeholder="Enter the IA Marks" name="iamarks">
            </div>
        </div>

        <div class="col-sm-6">
            <!-- text input -->
            <div class="form-group">
                <label>External Marks</label>
                <input required type="text" class="form-control" placeholder="Enter the External Marks" name="extmarks">
            </div>
        </div>

        <div class="col-sm-6">
            <!-- text input -->
            <div class="form-group">
                <label>Total Marks</label>
                <input required type="text" class="form-control" placeholder="Enter the Total Marks" name="totalmarks">
            </div>
        </div>

        <div class="col-sm-6">
            <!-- text input -->
            <div class="form-group">
                <label>CO1</label>
                <input required type="number" class="form-control" placeholder="Enter the CO1" name="co1">
            </div>
        </div>


        <div class="col-sm-6">
            <!-- text input -->
            <div class="form-group">
                <label>CO2</label>
                <input required type="number" class="form-control" placeholder="Enter the CO2" name="co2">
            </div>
        </div>


        <div class="col-sm-6">
            <!-- text input -->
            <div class="form-group">
                <label>CO3</label>
                <input required type="number" class="form-control" placeholder="Enter the CO3" name="co3">
            </div>
        </div>

    </div>

    <button class="btn btn-dark" type="submit">Submit</button>
</form>
</div>
<%- include("footer") %> 