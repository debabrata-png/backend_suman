<%- include("headerstud") %> 



<!-- Content Header (Page header) -->
<div class="content-header">
    <div class="container-fluid">
        <div class="row mb-2">
            <div class="col-sm-6">
                <h1 class="m-0 text-dark">View Enrolled Courses</h1>
            </div><!-- /.col -->
            <div class="col-sm-6">
                <ol class="breadcrumb float-sm-right">
                    <li class="breadcrumb-item"><a href="#">Home</a></li>
                    <li class="breadcrumb-item active">List Enrolled Courses</li>
                </ol>
            </div><!-- /.col -->
        </div><!-- /.row -->
    </div><!-- /.container-fluid -->
</div>
<!-- /.content-header -->

<!-- Main content -->
<section class="content">
    <div class="container-fluid">
        <!-- Small boxes (Stat box) -->
        <div class="row">
            <div class="col-md-12">
                <!-- general form elements disabled -->
                <div class="card card-dark">
                    <div class="card-header">
                        <h3 class="card-title">List Enrolled Courses</h3>
                    </div>
                    <!-- /.card-header -->
                    <div class="card-body">
                       
                        <table id="tbl-list-category" class="table table-bordered table-striped">
                            <thead>
                                <tr>
                                    <!-- <th>Coursecode</th> -->
                                    <th>Coursecode</th>
                                    <th>Course</th>
                                    <th>Faculty</th>
                                    <th>Faculty email</th>
                                    <!-- <th>Audience</th> -->
                                    <!-- <th>Semester</th>
                                    <th>Link</th> -->
                                    <th>Action</th>
                                </tr>
                            </thead>
                            <tbody>
                                <% categories.forEach(function(data){ %>
                                <tr>
                                    <!-- <td><%= data.id %></td> -->
                                    <!-- <td><%= data.coursecode %></td> -->
                                    <td> <%= data.coursecode %> </td>
                                    <td><%= data.course %></td>
                                    <td><%= data.name %></td>
                                    <td><%= data.user %></td>
                                    <!-- <td>Section <%= data.section %> Semester <%= data.semester %></td> -->
                                    <!-- <td><%= data.semester %></td> -->
                                    <!-- <td><%= data.link %></td> -->
                                    <td>
                                        
                                        <!-- <a href="/classboardstud/<%= data.id %>" class="btn btn-primary" target="_blank"><i class="fas fa-chalkboard" title="classboard"></i></a>
                                        <a href="/attend/<%= data.id %>" class="btn btn-primary" target="_blank"><i class="fas fa-video" title="join class"></i></a>
                                        <a href="/classchatstud/<%= data.id %>" class="btn btn-primary" target="_blank"><i class="fas fa-comments" title="chatboard"></i></a> -->
                                        <!-- <a href="http://naac2021.jdvu.ac.in/?criteria=3&Section=3.4.6&Subsection=Book&field1=<%= data.id %>" class="btn btn-primary" target="_blank"><i class="fas fa-upload"></i></a> -->
                                        <a href="/viewcfilesstud/<%= data.coursecode %>" class="btn btn-primary"><i class="far fa-file-alt" title="View course files"></i></a>
                                        <a href="/enrolclasslist?coursecode=<%= data.coursecode %>&email=<%= data.user %>" class="btn btn-primary"><i class="fas fa-video" title="class list"></i></a>
                                    </td>
                                </tr>
                                <% }) %>
                            </tbody>
                            <!-- <tfoot>
                                <tr>
                                    <th>#ID</th>
                                    <th>Name</th>
                                    <th>Status</th>
                                    <th>Action</th>
                                </tr>
                            </tfoot> -->
                        </table>
                    </div>
                    <!-- /.card-body -->
                </div>
            </div>
        </div>
        <!-- /.row -->
    </div><!-- /.container-fluid -->
</section>
<!-- /.content -->

<%- include("footer") %> 