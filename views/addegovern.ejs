<%- include("header") %> 

<div style="margin: 0px; padding: 40px;">
<h3>Add E-governance Areas</h1>
<br>
<br>
<form id="frm-add-category" name="frm-add-category" method="post" action="/addegovern">
    <div class="row">
        <div class="col-sm-6">
            <div class="form-group">
                <label>Areas of E governance</label>
                <select class="form-control" name="egovernareas">
                    <option value="Administration">Administration</option>
                    <option value="Finance and Accounts">Finance and Accounts</option>
                    <option value="Student Admission and Support">Student Admission and Support</option>
                    <option value="Examination">Examination</option>
                    

                </select>
            </div>
        </div>
        <div class="col-sm-6">
            <div class="form-group">
                <label>Year of Implementation</label>
                <select class="form-control" name="yearofimplement">
                    <option value="2015-16">2015-16</option>
                    <option value="2016-17">2016-17</option>
                    <option value="2017-18">2017-18</option>
                    <option value="2018-19">2018-19</option>
                    <option value="2019-20">2019-20</option>
                    <option value="2020-21">2020-21</option>
                    <option value="2021-22">2021-22</option>

                </select>
            </div>
        </div>


        <div class="col-sm-6">
            <!-- text input -->
            <div class="form-group">
                <label>Status</label>
                <input required type="text" class="form-control" placeholder="Enter the Status" name="status1">
            </div>
        </div>


        <div class="col-sm-6">
            <!-- text input -->
            <div class="form-group">
                <label>Comments</label>
                <input required type="text" class="form-control" placeholder="Enter the Comments" name="comments">
            </div>
        </div>
  
        
    </div>

    <button class="btn btn-dark" type="submit">Submit</button>
</form>
</div>
<%- include("footer") %> 