<%- include("header") %> 

<div style="margin: 0px; padding: 40px;">
<h3>Add Assignmnets Submitted</h1>
<br>
<br>
<form id="frm-add-category" name="frm-add-category" method="post" action="/addassignmentsub">
    <div class="row">
        <div class="col-sm-6">
            <div class="form-group">
                <label>Academic Year</label>
                <select class="form-control" name="year">
                    <option value="2015-16">2015-16</option>
                    <option value="2016-17">2016-17</option>
                    <option value="2017-18">2017-18</option>
                    <option value="2018-19">2018-19</option>
                    <option value="2019-20">2019-20</option>
                    <option value="2020-21">2020-21</option>
                    <option value="2021-22">2021-22</option>

                </select>
            </div>
        </div>
        <div class="col-sm-6">
            <!-- text input -->
            <div class="form-group">
                <label>Program Name</label>
                <input required type="text" class="form-control" placeholder="Enter program name" name="program">
            </div>
        </div>
        <div class="col-sm-6">
            <!-- text input -->
            <div class="form-group">
                <label>Course Name</label>
                <input required type="text" class="form-control" placeholder="Enter course name" name="course">
            </div>
        </div>
        <div class="col-sm-6">
            <!-- text input -->
            <div class="form-group">
                <label>Programwise Student Enrollment</label>
                <input required type="number" class="form-control" placeholder="Enter the total no. of enrollment in the program" name="totalenr">
            </div>
        </div>
        <div class="col-sm-6">
            <!-- text input -->
            <div class="form-group">
                <label>Assignments per course</label>
                <input required type="number" class="form-control" placeholder="Enter total no.of assignments per course" name="totalasgn">
            </div>
        </div>
        <div class="col-sm-6">
            <!-- text input -->
            <div class="form-group">
                <label>Assignments Submitted</label>
                <input required type="number" class="form-control" placeholder="Enter no.of assignments submitted" name="asgnsubmitted">
            </div>
        </div>

        <div class="col-sm-6">
            <!-- text input -->
            <div class="form-group">
                <label>Status</label>
                <select class="form-control" name="status1">
                    <option value="Submitted">Submitted</option>
                    <option value="In Process">In Process</option>
                    <option value="Incomplete Documentation">Incomplete Documentation</option>
                    <option value="Reviewed">Reviewed</option>
                    

                </select>
            </div>
        </div>
        <div class="col-sm-6">
            <!-- text input -->
            <div class="form-group">
                <label>Comments</label>
                <input required type="text" class="form-control" placeholder="Enter the Comments" name="comments">
            </div>
        </div>
  
        
    </div>

    <button class="btn btn-dark" type="submit">Submit</button>
</form>
</div>
<%- include("footer") %> 