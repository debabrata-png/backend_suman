<%- include("header") %> 

<div style="margin: 0px; padding: 40px;">
<h3>Add Student Qualifying to Higher Examination</h1>
<br>
<br>
<form id="frm-add-category" name="frm-add-category" method="post" action="/addhigherexam">
    <div class="row">
        <div class="col-sm-6">
            <div class="form-group">
                <label>Academic Year</label>
                <select class="form-control" name="year">
                    <option value="2015-16">2015-16</option>
                    <option value="2016-17">2016-17</option>
                    <option value="2017-18">2017-18</option>
                    <option value="2018-19">2018-19</option>
                    <option value="2019-20">2019-20</option>
                    <option value="2020-21">2020-21</option>
                    <option value="2021-22">2021-22</option>

                </select>
            </div>
        </div>
        <div class="col-sm-6">
            <!-- text input -->
            <div class="form-group">
                <label>Roll/Registration Number</label>
                <input required type="text" class="form-control" placeholder="Enter roll/registration number for exam" name="regno">
            </div>
        </div>
        <div class="col-sm-6">
            <!-- text input -->
            <div class="form-group">
                <label>Name of Student</label>
                <input required type="text" class="form-control" placeholder="Enter name of student qualifying the examination" name="studentname">
            </div>
        </div>
        <div class="form-group">
            <label>Examination Title</label>
            <select class="form-control" name="examname">
                <option value="NET">NET</option>
                <option value="SLET">SLET</option>
                <option value="GATE">GATE</option>
                <option value="GMAT">GMAT</option>
                <option value="CAT">CAT</option>
                <option value="GRE">GRE</option>
                <option value="JAM">JAM</option>
                <option value="IELTS">IELTS</option>
                <option value="TOEFL">TOEFL</option>
                <option value="Civil Services">Civil Services</option>
                <option value="State Government Examinations">State Government Examinations</option>
                <option value="Other Examinations">Other Examinations</option>

            </select>
        </div>

        <div class="col-sm-6">
            <!-- text input -->
            <div class="form-group">
                <label>Status</label>
                <input required type="text" class="form-control" placeholder="Enter the Status" name="status1">
            </div>
        </div>


        <div class="col-sm-6">
            <!-- text input -->
            <div class="form-group">
                <label>Comments</label>
                <input required type="text" class="form-control" placeholder="Enter the Comments" name="comments">
            </div>
        </div>
  
        
    </div>

    <button class="btn btn-dark" type="submit">Submit</button>
</form>
</div>
<%- include("footer") %> 