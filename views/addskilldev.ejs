<%- include("header") %> 

<div style="margin: 0px; padding: 40px;">
<h3>Add Skill Development Program</h1>
<br>
<br>
<form id="frm-add-category" name="frm-add-category" method="post" action="/addskilldev">
    <div class="row">
        <div class="col-sm-6">
            <!-- text input -->
            <div class="form-group">
                <label>Name of Capacity/Skill Development Program</label>
                <input required type="text" class="form-control" placeholder="Enter the name of skill development program" name="programname">
            </div>
        </div>
        <div class="col-sm-6">
            <!-- text input -->
            <div class="form-group">
                <label>Date of Implementation(DD-MM-YYY)</label>
                <input required type="date" class="form-control" placeholder="Enter the date of implementation" name="date">
            </div>
        </div>
        <div class="col-sm-6">
            <!-- text input -->
            <div class="form-group">
                <label>Number of students enrolled</label>
                <input required type="number" class="form-control" placeholder="Enter the number of students enrolled" name="noofstudenr">
            </div>
        </div>
        <div class="col-sm-6">
            <!-- text input -->
            <div class="form-group">
                <label>Name of Agency/Consultants involved</label>
                <input required type="text" class="form-control" placeholder="Enter the name of agency involved" name="agency">
            </div>
        </div>
  
        <div class="col-sm-6">
            <!-- text input -->
            <div class="form-group">
                <label>Contact details of Agency/Consultants involved</label>
                <input required type="text" class="form-control" placeholder="Enter the contact of agency involved" name="contactdetails">
            </div>
        </div>

        <div class="col-sm-6">
            <!-- text input -->
            <div class="form-group">
                <label>Status</label>
                <input required type="text" class="form-control" placeholder="Enter the Status" name="status1">
            </div>
        </div>


        <div class="col-sm-6">
            <!-- text input -->
            <div class="form-group">
                <label>Comments</label>
                <input required type="text" class="form-control" placeholder="Enter the Comments" name="comments">
            </div>
        </div>

    </div>

    <button class="btn btn-dark" type="submit">Submit</button>
</form>
</div>
<%- include("footer") %> 