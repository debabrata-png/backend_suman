<%- include("header") %> 

<div style="margin: 0px; padding: 40px;">
<h3>Add Book category</h1>
<br>
<br>
<form id="frm-add-category" name="frm-add-category" method="post" action="/login">
    <div class="row">
        <div class="col-sm-6">
            <!-- text input -->
            <div class="form-group">
                <label>Email</label>
                <input required type="text" class="form-control" placeholder="Enter email" name="email">
            </div>
        </div>
        <div class="col-sm-6">
            <!-- text input -->
            <div class="form-group">
                <label>Password</label>
                <input required type="password" class="form-control" placeholder="Enter password" name="password">
            </div>
        </div>
       
    </div>

    <button class="btn btn-dark" type="submit">Submit</button>
</form>
</div>
<%- include("footer") %> 