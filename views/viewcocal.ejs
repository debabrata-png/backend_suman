<%- include("header") %> 



<!-- Content Header (Page header) -->
<div class="content-header">
    <div class="container-fluid">
        <div class="row mb-2">
            <!-- <div class="col-sm-6">
                <h1 class="m-0 text-dark">View Course Outcome(CO)</h1>
            </div> -->
            <!-- /.col -->
            <div class="col-sm-8">
                <ol class="breadcrumb float-sm-left">
                    <li class="breadcrumb-item"><a href="/dashboard">Home</a></li>

                    <li class="breadcrumb-item active">List of Course Outcome Calculation</li>
                </ol>
            </div><!-- /.col -->
        </div><!-- /.row -->
    </div><!-- /.container-fluid -->
</div>
<!-- /.content-header -->

<!-- Main content -->
<section class="content">
    <div class="container-fluid">
        <!-- Small boxes (Stat box) -->
        <div class="row">
            <div class="col-md-12">
                <!-- general form elements disabled -->
                <div class="card card-dark">
                    <div class="card-header">
                        <h3 class="card-title">View Course Outcome Calculation</h3>
                    </div>
                    <!-- /.card-header -->
                    <div class="card-body">
                        <table>
                            <tr>
                                <!-- <td>
                                    <a class="btn btn-dark" href="/addcocal">+ Add New</a>
                                </td>
                                <td width="20px"></td> -->
                                <td>
                                    <a class="btn btn-dark" href="/addbulkcocal">+ Bulk Upload</a>
                                </td>
                                <!-- <td width="20px"></td>
                                <td>
                                    <a href="javascript:void(0)" onclick="if(confirm('Are you sure want to export ? ')){  $('#frm-export-category').submit()  }" class="btn btn-dark">Export to excel</a>
                                </td> -->
                            </tr>
                        </table>
                        

                        <br><br>
                        <table id="tbl-list-category" class="table table-bordered table-striped">
                            <thead>
                                <tr>
                                    <th>Course Code</th>
                                    <th>Course Name</th>
                                    <th>Program Code</th>
                                    <th>Program</th>
                                    <th>Exam Code</th>
                                    <th>Exam Name</th>
                                    <th>Semester</th>
                                    <th>IA Marks</th>
                                    <th>External Exam</th>
                                    <th>Total Marks</th>
                                    <th>CO1</th>
                                    <th>CO2</th>
                                    <th>CO3</th>
                                    <th>Regno</th>
                                    <th>Student</th>
                                    <th>Action</th>
                                </tr>
                            </thead>
                            <tbody>
                                <% categories.forEach(function(data){ %>
                                <tr>
                                    <!-- <td><%= data.id %></td> -->
                                    <td><%= data.coursecode %></td>
                                    <td><%= data.course %></td>
                                    <td><%= data.programcode %></td>
                                    <td><%= data.program %></td>
                                    <td><%= data.examcode %></td>
                                    <td><%= data.examname %></td>
                                    <td><%= data.semester %></td>
                                    <td><%= data.iamarks %></td>
                                    <td><%= data.extmarks %></td>
                                    <td><%= data.totalmarks %></td>
                                    <td><%= data.co1 %></td>
                                    <td><%= data.co2 %></td>
                                    <td><%= data.co3 %></td>
                                    <td><%= data.regno %></td>
                                    <td><%= data.student %></td>
                                    <td>
                                        <a href="/editcocal/<%= data.id %>" class="btn btn-primary"><i class="fa fa-edit"></i></a>
                                        <!-- <a href="http://naac2021.jdvu.ac.in/?criteria=1&Subsection=ValueAddedCourse&field1=<%= data.id %>" class="btn btn-primary" target="_blank"><i class="fas fa-upload"></i></a> -->
                                        <!-- <% link.forEach(function(data1){ %>
                                            <a href="<%= data1.uploadlink %>?criteria=1&Subsection=COPO&field1=<%= data.id %>&Year=<%= data.yop %>" class="btn btn-primary" target="_blank"><i class="fas fa-upload"></i></a>


                                            <% }) %> -->
                                        <form method="post" action="/deletecocal" id="frm-delete-category-<%= data.id %>">
                                        
                                            <input type="hidden" value="<%= data.id %>" name="category_id"/>

                                        </form>

                                    <a href="javascript:void(0)" onclick="if(confirm('Are you sure want to delete ? ')){  $('#frm-delete-category-<%= data.id %>').submit()  }" class="btn btn-danger"><i class="fa fa-trash"></i></a>
                                    </td>
                                </tr>
                                <% }) %>
                            </tbody>
                            <!-- <tfoot>
                                <tr>
                                    <th>#ID</th>
                                    <th>Name</th>
                                    <th>Status</th>
                                    <th>Action</th>
                                </tr>
                            </tfoot> -->
                        </table>
                    </div>
                    <!-- /.card-body -->
                </div>
            </div>
        </div>
        <!-- /.row -->
    </div><!-- /.container-fluid -->
</section>
<!-- /.content -->

<%- include("footer") %> 