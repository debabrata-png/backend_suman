<%- include("header") %> 



<!-- Content Header (Page header) -->
<div class="content-header">
    <div class="container-fluid">
        <div class="row mb-2">
            <div class="col-sm-6">
                <h1 class="m-0 text-dark">View Exam</h1>
            </div><!-- /.col -->
            <div class="col-sm-6">
                <ol class="breadcrumb float-sm-right">
                    <li class="breadcrumb-item"><a href="#">Home</a></li>
                    <li class="breadcrumb-item active">List Exam</li>
                </ol>
            </div><!-- /.col -->
        </div><!-- /.row -->
    </div><!-- /.container-fluid -->
</div>
<!-- /.content-header -->

<!-- Main content -->
<section class="content">
    <div class="container-fluid">
        <!-- Small boxes (Stat box) -->
        <div class="row">
            <div class="col-md-12">
                <!-- general form elements disabled -->
                <div class="card card-dark">
                    <div class="card-header">
                        <h3 class="card-title">List Exam</h3>
                    </div>
                    <!-- /.card-header -->
                    <div class="card-body">
                        <a class="btn btn-dark" href="/addexam">+ Add New Exam</a>
                        <br><br>
                        <table id="tbl-list-category" class="table table-bordered table-striped">
                            <thead>
                                <tr>
                                    <!-- <th>Exam Code</th> -->
                                    <th>Exam</th>
                                    <th>Program</th>
                                    <th>Exam date</th>
                                    <!-- <th>Class time</th> -->
                                    <th>Audience</th>
                                    <!-- <th>Semester</th>-->
                                    <th>Link</th> 
                                    <th>Action</th>
                                </tr>
                            </thead>
                            <tbody>
                                <% categories.forEach(function(data){ %>
                                <tr>
                                    <!-- <td><%= data.id %></td> -->
                                    <td><%= data.examname %></td>
                                    <td><%= data.programcode %>  <%= data.course %>  <%= data.coursecode %></td>
                                    <td><%= data.classdate %> to <%= data.enddate %></td>
                                    <!-- <td><%= data.classtime %></td> -->
                                    <td>Section <%= data.section %> Semester <%= data.semester %></td>
                                    <!-- <td><%= data.semester %></td> -->
                                    <td><%= data.link %></td>
                                    <td>
                                        <a href="/editexam/<%= data.id %>" class="btn btn-primary"><i class="fa fa-edit" title="edit exam"></i></a>
                                        <a href="/viewquestions/<%= data.id %>" class="btn btn-primary"><i class="fas fa-question-circle" title="Add questions"></i></a>
                                        <a href="/viewenrollexam/<%= data.id %>" class="btn btn-primary"><i class="fas fa-address-book" title="Enrolled student list"></i></a>
                                        <a href="/viewallfinalanswer?examid=<%= data.id %>" class="btn btn-primary"><i class="fas fa-signal"></i></a>
                                        <a href="/viewenrollfaculty/<%= data.id %>" class="btn btn-primary"><i class="fas fa-window-restore" title="Enrolled faculty list"></i></a>
                                        <a href="/getallfinalanswers?examid=<%= data.id %>" class="btn btn-primary"><i class="fas fa-database" title="tabulation sheet"></i></a>
                                        <a href="/getmyfinalanswers?examid=<%= data.id %>" class="btn btn-primary"><i class="fas fa-poll" title="my tabulation sheet"></i></a>
                                        <a href="/exportfinalscore?examid=<%= data.id %>" class="btn btn-primary"><i class="fas fa-poll" title="export score"></i></a>
                                        <% if(data.proctorlink == 'NA'){ %>
                                            <a href="https://meet.jit.si/<%= data.id %>" class="btn btn-primary" target="_blank"><i class="fas fa-video" title="start proctoring"></i></a>
                                            <% }else{ %>
                                                <a href="<%= data.proctorlink %>" class="btn btn-primary" target="_blank"><i class="fas fa-video" title="start proctoring"></i></a>
                                            <% } %>
                                        <form method="post" action="/deleteexam" id="frm-delete-category-<%= data.id %>">
                                        
                                            <input type="hidden" value="<%= data.id %>" name="category_id"/>

                                        </form>

                                    <a href="javascript:void(0)" onclick="if(confirm('Are you sure want to delete ? ')){  $('#frm-delete-category-<%= data.id %>').submit()  }" class="btn btn-danger"><i class="fa fa-trash" title="delete exam"></i></a>
                                    </td>
                                </tr>
                                <% }) %>
                            </tbody>
                            <!-- <tfoot>
                                <tr>
                                    <th>#ID</th>
                                    <th>Name</th>
                                    <th>Status</th>
                                    <th>Action</th>
                                </tr>
                            </tfoot> -->
                        </table>
                    </div>
                    <!-- /.card-body -->
                </div>
            </div>
        </div>
        <!-- /.row -->
    </div><!-- /.container-fluid -->
</section>
<!-- /.content -->

<%- include("footer") %> 